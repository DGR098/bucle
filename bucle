package com.estructurasControl.Repeticion;

public class For {

    public static void main(String[] args) {
        
        String sumas1 = "David ";
        String[] nombres = {"Pepe ", "Juanito ", "Ruperta ", "Daniel "}; // length 4
        for(int i = 0; i < nombres.length; i++){
            sumas1 += nombres[i];
            System.out.println(sumas1);
        }
      }
  }
